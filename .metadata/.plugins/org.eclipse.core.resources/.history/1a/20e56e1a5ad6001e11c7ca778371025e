package com.linkup.exceptions;

import org.springframework.web.bind.annotation.ControllerAdvice;

@ControllerAdvice
public class GlobalExceptions {

	public ResponseEntity<ErrorDetails> otherExceptionHandler(GlobalExceptions ue, WebRequest req){
		
		ErrorDetails error = new ErrorDetails(ue.getMessage() , req);
		
		
		return null;
	}

	
	
}
